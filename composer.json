  "type": "seat-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "recursive_tree",
      "email": "recursivetreemail@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
